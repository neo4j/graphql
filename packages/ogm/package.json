{
    "name": "@neo4j/graphql-ogm",
    "version": "1.0.0-beta.1",
    "description": "GraphQL powered OGM for Neo4j and Javascript applications",
    "keywords": [
        "neo4j",
        "graphql",
        "ogm"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/neo4j/graphql-tracker-temp/issues"
    },
    "homepage": "https://github.com/neo4j/graphql-tracker-temp/blob/master/README.md",
    "exports": {
        "require": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "tsc --project src/",
