        "upper-case-first": "^2.0.2"
    },
    "peerDependencies": {
        "graphql": "^15.0.0",
        "neo4j-driver": "^4.2.0"
    },
    "devDependencies": {
        "@types/jest": "26.0.8",
        "@types/node": "14.0.27",
        "graphql-tag": "2.11.0",
        "jest": "26.2.2",
        "npm-run-all": "^4.1.5",
        "randomstring": "1.1.5",
        "ts-jest": "26.1.4",
        "typescript": "3.9.7"
    }
