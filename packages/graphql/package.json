{
    "name": "@neo4j/graphql",
    "version": "1.0.0-alpha.6",
    "description": "A GraphQL to Cypher query execution layer for Neo4j and JavaScript GraphQL implementations",
    "keywords": [
        "neo4j",
        "graphql"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/neo4j/graphql-tracker-temp/issues"
    },
    "homepage": "https://github.com/neo4j/graphql-tracker-temp/blob/master/README.md",
    "exports": {
        "require": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "prepare": "npm run build:clean",
        "clean": "rimraf dist",
        "test": "run-s test:unit test:int test:tck",
        "test:unit": "jest unit",
        "test:unit:watch": "jest unit --watchAll",
        "test:tck": "jest tck",
        "test:tck:watch": "jest tck --watchAll",
        "test:int": "jest int",
        "test:int:watch": "jest int --watch",
        "setup:package-tests": "npm pack && mv *.tgz ../package-tests/ && cd ../package-tests/ && rimraf package && tar -xvzf *.tgz && npm install && npm run setup",
        "posttest:package-tests": "npm run cleanup:package-tests",
        "test:package-tests": "npm run setup:package-tests && cd ../package-tests/ && npm run test:all",
        "cleanup:package-tests": "cd ../package-tests/ && npm run cleanup && rimraf ./package/ && rimraf *.tgz",
        "build": "cd src/ && tsc && cd ../",
        "build:clean": "run-s clean build",
        "watch": "tsc -w",
        "start": "run-s build:clean watch",
        "pack": "npm pack"
    },
    "author": "Neo4j Inc.",
    "peerDependencies": {
        "graphql": "^15.0.0",
        "neo4j-driver": "^4.2.0"
    },
    "devDependencies": {
        "@graphql-tools/utils": "7.1.4",
        "@types/deep-equal": "1.0.1",
        "@types/faker": "5.1.7",
        "@types/is-uuid": "1.0.0",
        "@types/jest": "26.0.8",
        "@types/jsonwebtoken": "8.5.0",
        "@types/node": "14.0.27",
        "@types/pluralize": "0.0.29",
        "@types/randomstring": "1.1.6",
        "apollo-server": "2.21.0",
        "apollo-server-testing": "2.21.0",
        "faker": "5.2.0",
        "graphql": "15.3.0",
        "graphql-tag": "2.11.0",
        "is-uuid": "1.0.2",
        "jest": "26.2.2",
        "neo4j-driver": "4.2.0",
        "npm-run-all": "4.1.5",
        "randomstring": "1.1.5",
        "rimraf": "3.0.2",
        "ts-jest": "26.1.4",
        "typescript": "3.9.7"
    },
    "dependencies": {
        "@graphql-tools/merge": "^6.2.3",
        "@graphql-tools/schema": "^6.2.3",
        "@graphql-tools/utils": "7.6.0",
        "camelcase": "6.2.0",
        "deep-equal": "2.0.5",
        "dot-prop": "6.0.1",
        "graphql-compose": "7.21.4",
        "graphql-parse-resolve-info": "4.9.0",
        "jsonwebtoken": "8.5.1",
        "pluralize": "8.0.0",
        "upper-case-first": "^2.0.2"
    }
}
