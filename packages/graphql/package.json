{
    "name": "@neo4j/graphql",
    "version": "3.21.0",
    "description": "A GraphQL to Cypher query execution layer for Neo4j and JavaScript GraphQL implementations",
    "keywords": [
        "neo4j",
        "graphql",
        "server"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/neo4j/graphql/issues"
    },
    "homepage": "https://github.com/neo4j/graphql/tree/dev/packages/graphql",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**/*.ts",
        "dist/**/*.ts.map",
        "dist/**/*.js",
        "dist/**/*.js.map"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "clean": "cd src/ && tsc --build --clean",
        "test": "jest",
        "test:unit": "jest src --coverage=true -c jest.minimal.config.js",
        "test:unit:watch": "jest src --watch -c jest.minimal.config.js",
        "test:tck": "jest tests/tck -c jest.minimal.config.js",
        "test:tck:watch": "jest tck --watchAll -c jest.minimal.config.js",
        "test:int": "jest tests/integration",
        "test:int:watch": "jest int --watch",
        "test:e2e": "jest tests/e2e",
        "test:e2e:watch": "jest e2e --watch",
        "test:schema": "jest tests/schema -c jest.minimal.config.js",
        "test:schema:watch": "jest tests/schema --watch -c jest.minimal.config.js",
        "remove:graphql-plugin-auth": "node -p \"JSON.stringify({...require('./package.json')}, (k,v) => k === '@neo4j/graphql-plugin-auth' ? undefined : v, 2)\" > ./package.json.new && rm package.json && mv package.json.new package.json",
        "setup:package-tests": "yarn pack && mv *.tgz ../package-tests/ && cd ../package-tests/ && rimraf package && tar -xvzf *.tgz && cd package && npm run remove:graphql-plugin-auth && cd ../ && yarn install && yarn run setup",
        "posttest:package-tests": "yarn run cleanup:package-tests",
        "test:package-tests": "yarn run setup:package-tests && cd ../package-tests/ && yarn run test:all",
        "cleanup:package-tests": "cd ../package-tests/ && yarn run cleanup && rimraf ./package/ && rimraf *.tgz",
        "build": "tsc --build src/tsconfig.production.json",
        "build:clean": "run-s clean build",
        "watch": "tsc -w",
        "start": "run-s build:clean watch",
        "performance": "ts-node tests/performance/performance.ts"
    },
    "author": "Neo4j Inc.",
    "devDependencies": {
        "@apollo/gateway": "2.4.7",
        "@apollo/server": "4.7.3",
        "@faker-js/faker": "8.0.2",
        "@neo4j/graphql-plugin-auth": "^2.2.0",
        "@types/deep-equal": "1.0.1",
        "@types/is-uuid": "1.0.0",
        "@types/jest": "29.5.2",
        "@types/jsonwebtoken": "9.0.2",
        "@types/node": "18.15.1",
        "@types/pluralize": "0.0.29",
        "@types/randomstring": "1.1.8",
        "@types/semver": "7.5.0",
        "@types/supertest": "2.0.12",
        "@types/ws": "8.5.5",
        "dedent": "0.7.0",
        "graphql-middleware": "6.1.34",
        "graphql-tag": "2.12.6",
        "graphql-ws": "5.13.1",
        "is-uuid": "1.0.2",
        "jest": "29.5.0",
        "jest-extended": "4.0.0",
        "koa": "2.14.2",
        "koa-jwt": "4.0.4",
        "koa-router": "12.0.0",
        "libnpmsearch": "6.0.2",
        "mock-jwks": "1.0.10",
        "nock": "13.3.1",
        "npm-run-all": "4.1.5",
        "randomstring": "1.3.0",
        "rimraf": "5.0.1",
        "supertest": "6.3.3",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "typescript": "5.1.3",
        "ws": "8.13.0"
    },
    "dependencies": {
        "@apollo/subgraph": "^2.2.3",
        "@graphql-tools/merge": "^8.3.3",
        "@graphql-tools/resolvers-composition": "^6.5.3",
        "@graphql-tools/schema": "9.0.19",
        "@graphql-tools/utils": "^9.0.0",
        "@neo4j/cypher-builder": "~0.5.1",
        "camelcase": "^6.3.0",
        "debug": "^4.3.4",
        "deep-equal": "^2.0.5",
        "dot-prop": "^6.0.1",
        "graphql-compose": "^9.0.8",
        "graphql-parse-resolve-info": "^4.12.3",
        "graphql-relay": "^0.10.0",
        "jose": "^4.13.1",
        "pluralize": "^8.0.0",
        "semver": "^7.3.7",
        "uuid": "^9.0.0"
    },
    "peerDependencies": {
        "graphql": "^16.0.0",
        "neo4j-driver": "^4.1.0 || ^5.0.0"
    }
}
