{
    "name": "@neo4j/introspector",
    "version": "2.0.0-beta.1",
    "description": "Introspect a Neo4j database model/schema",
    "keywords": [
        "neo4j",
        "graphql",
        "schema"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/neo4j/graphql/issues"
    },
    "homepage": "https://github.com/neo4j/graphql/tree/dev/packages/introspector",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**/*.ts",
        "dist/**/*.ts.map",
        "dist/**/*.js",
        "dist/**/*.js.map"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "clean": "cd src/ && tsc --build --clean",
        "test": "jest",
        "test:unit": "jest src",
        "test:unit:watch": "jest src --watch",
        "test:int": "jest tests/integration",
        "test:int:watch": "jest tests/integration --watch",
        "build": "tsc --build tsconfig.production.json",
        "build:clean": "yarn clean && yarn build"
    },
    "author": "Neo4j Inc.",
    "devDependencies": {
        "@neo4j/graphql": "^4.0.0-beta.2",
        "@types/jest": "29.5.3",
        "@types/node": "18.15.1",
        "@types/pluralize": "0.0.30",
        "jest": "29.6.3",
        "ts-jest": "29.1.1",
        "typescript": "5.1.6"
    },
    "dependencies": {
        "camelcase": "^6.3.0",
        "debug": "^4.3.4",
        "pluralize": "^8.0.0"
    },
    "peerDependencies": {
        "neo4j-driver": "^5.8.0"
    }
}
