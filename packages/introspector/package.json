{
    "name": "@neo4j/introspector",
    "version": "1.0.0",
    "description": "Introspect a Neo4j database model/schema",
    "keywords": [
        "neo4j",
        "graphql",
        "schema"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/neo4j/graphql/issues"
    },
    "homepage": "https://github.com/neo4j/graphql/tree/master/packages/introspector",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "clean": "cd src/ && tsc --build --clean",
        "test": "jest",
        "test:unit": "jest src",
        "test:unit:watch": "jest src --watch",
        "test:int": "jest tests/integration",
        "test:int:watch": "jest tests/integration --watch",
        "build": "tsc --project src/",
        "build:clean": "yarn clean && yarn build"
    },
    "author": "Neo4j Inc.",
    "devDependencies": {
        "@neo4j/graphql": "^2.4.0",
        "@types/jest": "26.0.8",
        "@types/node": "14.0.27",
        "@types/pluralize": "0.0.29",
        "jest": "26.2.2",
        "ts-jest": "26.1.4",
        "typescript": "3.9.7"
    },
    "dependencies": {
        "camelcase": "^6.2.0",
        "debug": "^4.3.2",
        "pluralize": "^8.0.0"
    },
    "peerDependencies": {
        "neo4j-driver": "^4.1.0"
    }
}
